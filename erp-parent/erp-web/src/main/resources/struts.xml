<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="erp" extends="struts-default" namespace="/">
		<action name="depAction_*" class="depAction" method="{1}"></action>
		<action name="empAction_*" class="empAction" method="{1}"></action>
		<action name="goodsAction_*" class="goodsAction" method="{1}"></action>
		<action name="goodsTypeAction_*" class="goodsTypeAction" method="{1}"></action>
		<action name="loginAction_*" class="loginAction" method="{1}"></action>
		<action name="menuAction_*" class="menuAction" method="{1}"></action>
		<action name="supplierAction_*" class="supplierAction" method="{1}"></action>
		<action name="ordersAction_*" class="ordersAction" method="{1}"></action>
		<action name="orderDetailAction_*" class="orderDetailAction" method="{1}"></action>
		<action name="storeAction_*" class="storeAction" method="{1}"></action>
		<action name="storeDetailAction_*" class="storeDetailAction" method="{1}"></action>
		<action name="storeOperAction_*" class="storeOperAction" method="{1}"></action>
		<action name="reportAction_*" class="reportAction" method="{1}"></action>
		<action name="roleAction_*" class="roleAction" method="{1}"></action>
	</package>
</struts>